{
  "version": 2,
  "cleanUrls": true,
  "domains": ["threadtwin.com", "www.threadtwin.com"],
  "redirects": [
    {
      "source": "https://www.threadtwin.com/:path*",
      "destination": "https://threadtwin.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 